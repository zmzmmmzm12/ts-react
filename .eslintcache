floor(Math.random()*1000)+2000);\r\n            setState('ready');\r\n            setMessage('초록색이 되면 눌러주세요');\r\n        }\r\n        else if(state==='ready'){\r\n            if(timeout.current){\r\n                clearTimeout(timeout.current);\r\n            }\r\n            setState('waiting');\r\n            setMessage('너무 성급하시군요! 초록색이 된 후에 클릭하세요');\r\n        }\r\n        else if(state==='now'){\r\n            endTime.current=new Date().getTime();\r\n            setState('waiting');\r\n            setMessage('클릭해서 시작하세요');\r\n            setResult((prevResult)=>{\r\n                return [...prevResult, endTime.current-startTime.current];\r\n            })\r\n        }\r\n    }, [state])\r\n\r\n    const onReset=useCallback(()=>{\r\n        setResult([]);\r\n    }, [state])\r\n\r\n    const renderAverage=()=>{\r\n        return result.length===0\r\n            ?null\r\n            :<>\r\n                <div>평균시간: {result.reduce((a,c)=>a+c)/result.length}ms</div>\r\n                <button onClick={onReset}>리셋</button>\r\n            </>\r\n    }\r\n\r\n    return(\r\n        <>\r\n            <div\r\n            id='screen'\r\n            className={state}\r\n            onClick={onClickScreen}>\r\n                {message}\r\n            </div>\r\n            {renderAverage()}\r\n        </>\r\n    )\r\n}\r\n\r\nexport default ResponseCheck;","C:\\Users\\82107\\Desktop\\갭이어_사전준비\\강의따라하기\\웹게임을 만들며 배우는 리액트\\tsReactTest\\ts-react\\src\\comp\\RSP.tsx",["40"],{"ruleId":"41","severity":1,"message":"42","line":2,"column":27,"nodeType":"43","messageId":"44","endLine":2,"endColumn":36},{"ruleId":"45","severity":1,"message":"46","line":24,"column":8,"nodeType":"47","endLine":24,"endColumn":15,"suggestions":"48"},{"ruleId":"41","severity":1,"message":"49","line":5,"column":10,"nodeType":"43","messageId":"44","endLine":5,"endColumn":24},{"ruleId":"45","severity":1,"message":"50","line":78,"column":8,"nodeType":"47","endLine":78,"endColumn":23,"suggestions":"51"},{"rule